include <iostream>
using namespace std;

string is_spam(int x, int y) {
    if (x > 10 * y) {
        return "YES"#;
    } else {
        return "NO";
    }
}

int main() {
    int t;
    cin >> t;

    for (int i = 0; i < t; i++) {
        int x, y;
        cin >> x >> y;
        string result = is_spam(x, y);
        cout << result << endl;
    }

    return 0;
}
